<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Og\Bundle\AdminBundle\Entity\CustomersRepository" name="Og\Bundle\AdminBundle\Entity\Customers" table="customers">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="CustomerId" type="integer" column="CustomerId">
      <generator strategy="AUTO"/>
    </id>
    <field name="LastName" type="string" column="LastName" length="45"/>
    <field name="FirstName" type="string" column="FirstName" length="45"/>
    <field name="Company" type="string" column="Company" length="45"/>
    <field name="AdressField1" type="string" column="AdressField1" length="45"/>
    <field name="AdressField2" type="string" column="AdressField2" length="45"/>
    <field name="ZipCode" type="string" column="ZipCode" length="45"/>
    <field name="City" type="string" column="City" length="45"/>
    <field name="PhoneComment" type="string" column="PhoneComment" length="45"/>
    <field name="Mobile" type="string" column="Mobile" length="45"/>
    <field name="BusinessPhone" type="string" column="BusinessPhone" length="45"/>
    <field name="Fax" type="string" column="Fax" length="45"/>
    <field name="Email1" type="string" column="Email1" length="45"/>
    <field name="Email2" type="string" column="Email2" length="45"/>
    <field name="Email3" type="string" column="Email3" length="45"/>
    <field name="Remark" type="string" column="Remark" length="45"/>
    <field name="Title" type="integer" column="Title" />
    <field name="Country" type="integer" column="Country" />
    <field name="State" type="integer" column="State" />
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>